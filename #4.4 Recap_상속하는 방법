type PlayerA = {
  name : string
}
// type 상속하는 법
type PlayerAA = PlayerA & {
  lastName : string
}
const playerA : PlayerAA = {
  name : "nico",
  lastName: "xxx"
}

////

interface PlayerB {
  name : string
}
// interface 상속하는 법
interface PlayerBB extends PlayerB {
  lastName : string
}
// interface 에 property를 추가하는 방법
interface PlayerBB {
  health : number
}
const playerB : PlayerBB = {
  name: "nico",
  lastName : "xxx",
  health : 1
}


